<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:textColorPrimary">@color/textColorPrimary</item>
        <item name="android:textColorSecondary">@color/textColorSecondary</item>

        <item name="android:colorBackground">@color/md_white</item>

        <item name="themedMenuGroupDrawable">@drawable/ic_group_black_24dp</item>
        <item name="themedMenuVisibilityDrawable">@drawable/ic_visibility_black_24dp</item>
        <item name="themedAboutInfoDrawable">@drawable/ic_info_outline_black_24dp</item>
        <item name="themedAccountInfoDrawable">@drawable/ic_person_black_24dp</item>
        <item name="themedScheduleDrawable">@drawable/ic_schedule_black_24dp</item>
        <item name="themedCalendarDrawable">@drawable/ic_event_black_24dp</item>
    </style>

    <!-- ################# -->
    <!-- #   THEME NUIT  # -->
    <!-- ################# -->
    <style name="AppThemeNight" parent="Theme.AppCompat">
        <item name="colorPrimary">@color/backgroundNight</item>
        <item name="colorPrimaryDark">@color/backgroundNightDark</item>
        <item name="colorAccent">@color/colorAccentNight</item>
        <item name="android:textColorPrimary">@color/textColorPrimaryNight</item>
        <item name="android:textColorSecondary">@color/textColorSecondaryNight</item>

        <item name="android:colorBackground">@color/backgroundNightDark</item>

        <item name="themedMenuGroupDrawable">@drawable/ic_group_white_24dp</item>
        <item name="themedMenuVisibilityDrawable">@drawable/ic_visibility_white_24dp</item>
        <item name="themedAboutInfoDrawable">@drawable/ic_info_outline_white_24dp</item>
        <item name="themedAccountInfoDrawable">@drawable/ic_person_white_24dp</item>
        <item name="themedScheduleDrawable">@drawable/ic_schedule_white_24dp</item>
        <item name="themedCalendarDrawable">@drawable/ic_event_white_24dp</item>
    </style>

    <style name="AppThemeNight.NoActionBar" parent="AppThemeNight">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.NoActionBar" parent="AppTheme">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.NoActionBar.SplashScreen" parent="AppTheme.NoActionBar">
        <item name="android:windowTranslucentStatus">true</item>
        <item name="android:windowBackground">@drawable/splashscreen_background</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="Widget.CardContent" parent="android:Widget">
        <item name="android:paddingLeft">16dp</item>
        <item name="android:paddingRight">16dp</item>
        <item name="android:paddingTop">20dp</item>
        <item name="android:paddingBottom">20dp</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="Widget.CardContentSmall" parent="Widget.CardContent">
        <item name="android:paddingTop">18dp</item>
        <item name="android:paddingBottom">18dp</item>
    </style>

    <style name="Widget.CardContentRecycler" parent="Widget.CardContent">
        <item name="android:paddingLeft">0dp</item>
        <item name="android:paddingRight">0dp</item>
        <item name="android:paddingTop">0dp</item>
        <item name="android:paddingBottom">0dp</item>
    </style>

    <style name="AppThemeButton" parent="Theme.AppCompat">
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="MyOverflowText" parent="TextAppearance.AppCompat.Small">
        <item name="android:textColor">@color/md_red_500</item>
    </style>

    <style name="FullWidthEditText" parent="Widget.AppCompat.EditText">
        <item name="android:background">@null</item>
        <item name="android:textSize">16sp</item>
        <item name="android:minHeight">?listPreferredItemHeight</item>
        <item name="android:paddingBottom">20dp</item>
        <item name="android:paddingTop">20dp</item>
        <item name="android:paddingEnd">?listPreferredItemPaddingRight</item>
        <item name="android:paddingStart">?listPreferredItemPaddingLeft</item>
    </style>

    <style name="FullWidthEditTextLayout" parent="FullWidthEditText">
        <item name="android:paddingBottom">10dp</item>
        <item name="android:paddingTop">0dp</item>
        <item name="android:layout_marginBottom">0dp</item>
    </style>

    <style name="FullWidthEditTextIcon">
        <item name="android:minHeight">?listPreferredItemHeight</item>
        <item name="android:paddingBottom">17dp</item>
        <item name="android:paddingTop">17dp</item>
        <item name="android:paddingStart">?listPreferredItemPaddingLeft</item>
    </style>
    <style name="FullWidthEditTextIconMiddle" parent="FullWidthEditTextIcon">
        <item name="android:paddingBottom">0dp</item>
    </style>

